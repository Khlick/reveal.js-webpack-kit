{
  "name": "revealjs-webpack-sauce",
  "version": "1.0.0",
  "description": "Webpack-powered revealjs presentation starter kit",
  "main": "index.js",
  "repository": "https://github.com/gcalmettes/revealjs-webpack-sauce",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV='dev-server' webpack-dev-server",
    "build": "NODE_ENV='production' FONT_AWESOME='svg' webpack --config 'webpack.config.js'",
    "build-fa-css": "NODE_ENV='production-fa-css' FONT_AWESOME='css' webpack --config 'webpack.config.js'",
    "build-web": "NODE_ENV='production-web' FONT_AWESOME='css' webpack --config 'webpack.config.js'",
    "build-server-rendering": "NODE_ENV='production-server-rendering' webpack --config 'webpack.config.js'"
  },
  "author": "Guillaume Calmettes",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "google-fonts-webpack-plugin": "^0.4.4",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "node-fetch": "^1.7.3",
    "node-sass": "^4.9.0",
    "path": "^0.12.7",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.12.0",
    "webpack-conditional-loader": "^1.0.12",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "animate.css": "^3.6.1",
    "highlight.js": "^9.12.0",
    "jquery": "^3.3.1",
    "mathjax": "^2.7.4",
    "reveal.js": "^3.6.0",
    "reveal.js-menu": "^1.1.2",
    "reveal_external": "^1.3.0",
    "reveald3": "^1.4.3"
  }
}
