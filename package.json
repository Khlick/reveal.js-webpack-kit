{
  "name": "reveal.js-webpack-kit",
  "version": "2.0.3",
  "description": "webpack-powered reveal.js presentations",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "live-fa-css": "set NODE_ENV=dev-server&& set FONT_AWESOME='css'&& webpack-dev-server --mode development",
    "live-fa-svg": "set NODE_ENV=dev-server&& set FONT_AWESOME='svg'&& webpack-dev-server --mode development",
    "dev-fa-css": "set NODE_ENV=development&& set FONT_AWESOME='css'&& webpack --mode development",
    "dev-fa-svg": "set NODE_ENV=development&& set FONT_AWESOME='svg'&& webpack --mode development",
    "build-fa-css": "set NODE_ENV=production&& set FONT_AWESOME='css'&& webpack --mode production",
    "build-fa-svg": "set NODE_ENV=production&& set FONT_AWESOME='svg'&& webpack --mode production",
    "build-web": "set NODE_ENV='production-web-css'&& set FONT_AWESOME='css'&& webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcalmettes/reveal.js-webpack-kit.git"
  },
  "keywords": [
    "webpack",
    "reveal.js"
  ],
  "author": "Guillaume Calmettes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gcalmettes/reveal.js-webpack-kit/issues"
  },
  "homepage": "https://github.com/gcalmettes/reveal.js-webpack-kit#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "file-loader": "^1.1.11",
    "google-fonts-plugin": "2.0.2",
    "html-webpack-include-assets-plugin": "^1.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "highlight.js": "^9.15.6",
    "reveal.js": "^3.8.0",
    "reveal.js-menu": "^1.2.0",
    "reveal_external": "^1.3.0",
    "reveald3": "^1.4.7"
  }
